/**
 * 
 */

 function fn_submit(){
	 var fn = document.reserv;
	 if(fn.resvno.value == ""){
		 alert("접종예약번호가 입력되지 않았습니다.");
		 fn.resvno.focus();
		 return false;
	 }
	 if(fn.jumin.value == ""){
		 alert("주빈번호가 입력되지 않았습니다.");
		 fn.resvno.focus();
		 return false;
	 }
	 if(fn.resvno.value == ""){
		 alert("백신코드가 입력되지 않았습니다.");
		 fn.resvno.focus();
		 return false;
	 }
	 if(fn.resvno.value == ""){
		 alert("병원코드가 입력되지 않았습니다.");
		 fn.resvno.focus();
		 return false;
	 }
	 if(fn.resvno.value == ""){
		 alert("예약일자가 입력되지 않았습니다.");
		 fn.resvno.focus();
		 return false;
	 }
	 if(fn.resvno.value == ""){
		 alert("예약시간이 입력되지 않았습니다.");
		 fn.resvno.focus();
		 return false;
	 }
	 
	 
	 fn.submit();
 }
 
 function fn_reset(){
	 
	 
 }