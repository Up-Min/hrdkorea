/**
 * 
 */

 function signup(){
	 var f = document.signup_form;
	 
	 if(f.user_id == ''){
		 alert ("아이디를 입력해 주십시오.");
		 return false;
	 }
	 if(f.user_id == ''){
		 alert ("아이디를 입력해 주십시오.");
		 return false;
	 }
	 if(f.user_email == ''){
		 alert ("이메일 주소를 입력해 주십시오.");
		 return false;
	 }
	 
	 
 }